# Microshare API Configuration
MICROSHARE_USERNAME=your_username@company.com
MICROSHARE_PASSWORD=your_password_here
MICROSHARE_API_KEY=YOUR-API-KEY-HERE

# Azure Event Hub Configuration
EVENT_HUB_CONNECTION_STRING=Endpoint=sb://your-eventhub.servicebus.windows.net/;SharedAccessKeyName=RootManageSharedAccessKey;SharedAccessKey=your_key_here;EntityPath=your-event-hub-name

# Application Settings
LOG_LEVEL=INFO

# Microshare Identity Filter (matches substring in owner.org)
# Example: "ACME" matches "com.company.ACME"
MICROSHARE_IDENTITY=YOUR_IDENTITY

# People Counter Configuration
MICROSHARE_PC_DISCOVERY_VIEW_ID=your_pc_discovery_view_id
MICROSHARE_PC_DASHBOARD_VIEW_ID=your_pc_dashboard_view_id
MICROSHARE_PC_DATA_CONTEXT=["people"]

# Snapshot Configuration
MICROSHARE_SNAPSHOT_DASHBOARD_VIEW_ID=your_snapshot_dashboard_view_id
MICROSHARE_SNAPSHOT_DATA_CONTEXT=["context1","context2","context3"]
MICROSHARE_SNAPSHOT_CATEGORY=space
MICROSHARE_SNAPSHOT_METRIC=occupancy

# Location Mapping
# If people counter uses "PREFIX Location" and snapshot uses "Location", set PREFIX here
# Example: "COMPANY" removes "COMPANY " prefix from location names
MICROSHARE_LOCATION_PREFIX=YOUR_PREFIX
